{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "repositories": [
        { "type": "vcs", "url": "https://github.com/open-sausages/silverstripe-framework" },
        { "type": "vcs", "url": "https://github.com/open-sausages/silverstripe-admin" }
    ],
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/assets": "1.3.x-dev",
        "silverstripe/config": "1.0.x-dev",
        "silverstripe/framework": "dev-experimental/gridfield as 4.3.x-dev",
        "silverstripe/admin": "dev-experimental/gridfield as 1.3.x-dev",
        "silverstripe/asset-admin": "1.3.x-dev",
        "silverstripe/campaign-admin": "1.3.x-dev",
        "silverstripe/cms": "4.3.x-dev",
        "silverstripe/errorpage": "1.3.x-dev",
        "silverstripe/graphql": "2.1.x-dev",
        "silverstripe/reports": "4.3.x-dev",
        "silverstripe/siteconfig": "4.3.x-dev",
        "silverstripe/versioned": "1.3.x-dev",
        "silverstripe-themes/simple": "~3.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "4.x-dev": "4.3.x-dev"
        },
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
